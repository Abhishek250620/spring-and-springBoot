package com.constructor.Employee;

import java.util.Map;

public class Employee {
	private  int id;
	private String name;
	private double salary;
	private Map<String, String> project;
	public Employee(int id, String name, double salary, Map<String, String> project) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
		this.project = project;
	}
	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + ", salary=" + salary + ", project=" + project + "]";
	}
}
